<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y6"
        android:background="@drawable/bg_shap_button_19"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:background="@drawable/bg_shap_button_20"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/three_level_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x8"
                android:layout_marginRight="@dimen/x8"
                android:text="7.特需医疗服务之比"
                android:textColor="#333333"
                android:textSize="@dimen/x13" />

            <TextView
                android:id="@+id/three_level_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="@dimen/x8"
                android:layout_marginRight="@dimen/x8"
                android:text=""
                android:textColor="#333333"
                android:textSize="@dimen/x13"
                android:visibility="visible" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/forth_level_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>

    </LinearLayout>

</LinearLayout>